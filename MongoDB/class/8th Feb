

USE org123;

CREATE TABLE Student (
    sno INT PRIMARY KEY,
    sname VARCHAR(20),
    age INT
);

INSERT INTO Student(sno, sname, age) 
VALUES (5, 'skit', 17),
       (7, 'samya', 18),
       (6, 'samm', 16);

SELECT * FROM Student;

CREATE TABLE Course (
    cno INT PRIMARY KEY,
    cname VARCHAR(20)
);

INSERT INTO Course(cno, cname) 
VALUES (101, 'c'),
       (102, 'c++'),
       (103, 'DBMS'),
       (104, 'c#'),
       (105, 'java'),
       (106, 'OS');

SELECT * FROM Course;

CREATE TABLE Enroll (
    sno INT,
    cno INT,
    jdate DATE,
    PRIMARY KEY(sno, cno),
    FOREIGN KEY(sno) REFERENCES Student(sno) ON DELETE CASCADE ON UPDATE CASCADE,
    FOREIGN KEY(cno) REFERENCES Course(cno) ON DELETE CASCADE ON UPDATE CASCADE
);

INSERT INTO Enroll(sno, cno, jdate) 
VALUES (1, 101, '2021-06-05'),
       (1, 102, '2021-06-05'),
       (2, 103, '2021-06-06');

SELECT * FROM Enroll;


UPDATE Student SET sno = 10 WHERE sname = 'skit';


SELECT * FROM Enroll;


DELETE FROM Student WHERE sname = 'samya';


SELECT * FROM Enroll;

INSERT INTO Enroll(sno, cno, jdate) 
VALUES(2, 105, '2021-05-05');
SELECT * FROM Enroll;
DESCRIBE Enroll;


